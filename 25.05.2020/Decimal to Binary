import java.io.*;
import java.util.*;
import java.math.*;
public class Decimal {
    public static void main(String args[] ) throws Exception {
        int num,count=0;
        //String x = "";
        Scanner s = new Scanner(System.in);
        System.out.print("Enter a Number:");
        num = s.nextInt();
        while(num > 0)
        {
            int rem = num % 2;
            num = num / 2;
        }
        System.out.println(num);
    }
}
